{
    "name": "spocr",
    "displayName": "spocr",
    "description": "vscode extension for the beautiful spocr",
    "version": "0.0.1",
    "publisher": "nuetzliches",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onView:spocrExplorer",
        "onLanguage:sql",
        "onLanguage:csharp"
    ],
    "main": "./out/extension",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "spocr",
                    "title": "SpocR",
                    "icon": "media/spocr.svg"
                }
            ]
        },
        "views": {
            "spocr": [
                {
                    "id": "spocr.spocrExplorer:spocr",
                    "name": "Explorer",
                    "when": "spocr:enabled && spocr:spocrExplorer == spocr"
                }
            ],
            "explorer": [
                {
                    "id": "spocr.spocrExplorer:explorer",
                    "name": "Spocr",
                    "when": "spocr:enabled && spocr:spocrExplorer == explorer"
                }
            ],
            "scm": [
                {
                    "id": "spocr.spocrExplorer:scm",
                    "name": "Spocr",
                    "when": "spocr:enabled && spocr:spocrExplorer == scm"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "vscode-uri": "^1.0.6"
    }
}
